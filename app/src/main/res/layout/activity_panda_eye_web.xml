<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/colorPrimary">
        <ImageView
            android:id="@+id/pandaeyeweb_finsh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@mipmap/play_fullscree_back"/>
    </RelativeLayout>
    <WebView
        android:id="@+id/pandaeye_webView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9">
    </WebView>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <ImageView
            android:id="@+id/collect_bo_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="150dp"
            android:src="@mipmap/collect_no"
            android:layout_marginBottom="5dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="150dp" />

        <ImageView
            android:id="@+id/pandaeye_shape_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="150dp"
            android:layout_marginBottom="5dp"
            android:src="@mipmap/pe_share_img"
            android:layout_alignParentRight="true"
            android:layout_marginRight="150dp" />
    </RelativeLayout>


</LinearLayout>

